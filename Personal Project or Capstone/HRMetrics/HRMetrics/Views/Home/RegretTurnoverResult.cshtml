@model HRMetrics.Models.TurnoverResponse

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>RegretTurnoverResult</title>
</head>
<body>
    <div>
        <h3>Total Turnover Summary</h3>
        <p>Starting Headcount: @Model.StartHeadcount</p>
        <p>Ending Headcount: @Model.EndHeadcount</p>
        <p>Average Headcount: @Model.AverageHeadcount </p>

        <p>Total (Voluntary) Regrettable Terminations: @Model.VolunataryRegrettableTerminations</p>
        <p>Regrettable Turnover Rate: @Model.RegrettableTurnover.ToString("P")</p>
         
    </div>
</body>
</html>
